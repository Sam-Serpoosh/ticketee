<%= form_for([@project, @ticket], :html => { :multipart => true }) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %><br />
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %><br />
  </p>
  <p>
    <%= f.label :asset, "File" %><br />
    <%= f.file_field :asset %><br />
  </p>

  <%= f.submit %>

<% end %>
